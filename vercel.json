{
    "version": 2,
    "builds": [
      {
        "src": "api/greetings.go",
        "use": "@vercel/go"
      }
    ],
    "routes": [
      {
        "src": "/api/Salam/greetings",
        "dest": "api/greetings.go"
      },
      {
        "src": "/api/Salam/greetings/:id",
        "dest": "api/greetings.go"
      }
    ]
  }
  